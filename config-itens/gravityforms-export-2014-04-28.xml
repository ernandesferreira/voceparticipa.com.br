<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.7.13">
	<form labelPlacement="left_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Sugestões]]></title>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[Enviar]]></text>
		</button>
		<fields>
			<field id="1" isRequired="1" size="large" type="email">
				<cssClass><![CDATA[email-form]]></cssClass>
				<label><![CDATA[E-Mail]]></label>
			</field>
			<field id="2" isRequired="1" size="large" type="text">
				<label><![CDATA[Sugestão]]></label>
			</field>
		</fields>
		<gquizGrading><![CDATA[none]]></gquizGrading>
		<gquizConfirmationFail><![CDATA[<strong>Quiz Results:</strong> You Failed!
<strong>Correct Answers:</strong> {quiz_score}
<strong>Percentage:</strong> {quiz_percent}%]]></gquizConfirmationFail>
		<gquizConfirmationPass><![CDATA[<strong>Quiz Results:</strong> You Passed!
<strong>Correct Answers:</strong> {quiz_score}
<strong>Percentage:</strong> {quiz_percent}%]]></gquizConfirmationPass>
		<gquizConfirmationLetter><![CDATA[<strong>Quiz Grade:</strong> {quiz_grade}
<strong>Correct Answers:</strong> {quiz_score}
<strong>Percentage:</strong> {quiz_percent}%]]></gquizConfirmationLetter>
		<gquizGrades>
			<gquizGrade>
				<text><![CDATA[A]]></text>
				<value><![CDATA[90]]></value>
			</gquizGrade>
			<gquizGrade>
				<text><![CDATA[B]]></text>
				<value><![CDATA[80]]></value>
			</gquizGrade>
			<gquizGrade>
				<text><![CDATA[C]]></text>
				<value><![CDATA[70]]></value>
			</gquizGrade>
			<gquizGrade>
				<text><![CDATA[D]]></text>
				<value><![CDATA[60]]></value>
			</gquizGrade>
			<gquizGrade>
				<text><![CDATA[E]]></text>
				<value><![CDATA[0]]></value>
			</gquizGrade>
		</gquizGrades>
		<gquizPassMark><![CDATA[50]]></gquizPassMark>
		<cssClass><![CDATA[label-form]]></cssClass>
		<confirmations>
			<confirmation id="535d9672bccd8" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Obrigado por entrar em contato conosco! Nós entraremos em contato com você em breve.]]></message>
			</confirmation>
		</confirmations>
		<notifications>
			<notification id="535d9672bc508">
				<to><![CDATA[1]]></to>
				<name><![CDATA[Notificação de Sugestão]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[field]]></toType>
				<subject><![CDATA[#{entry_id} {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
				<from><![CDATA[{embed_url}]]></from>
				<fromName><![CDATA[{Sugestão:2}]]></fromName>
			</notification>
		</notifications>
	</form>
</forms>